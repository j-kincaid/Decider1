{
  
  "functions": {
    "api/users/**/*.js": {
      "maxDuration": 10
    },

    "api/test.js": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },

  "builds": [
    {
      "src": "artfave/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "artfave/wsgi.py"
    }
  ]
}
